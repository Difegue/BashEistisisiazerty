<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<% var valid =typeof toSay != 'undefined' && toSay.hasOwnProperty("id") && toSay.hasOwnProperty("name") && toSay.hasOwnProperty("insulte") && toSay.hasOwnProperty("recommended_action") && toSay.hasOwnProperty("proportion") && toSay.hasOwnProperty("capacity") && toSay.hasOwnProperty("context") && toSay.hasOwnProperty("reproche") && toSay.hasOwnProperty("cause");%>
